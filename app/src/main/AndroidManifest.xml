<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="eo.cn.yxwuliu">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".activity.CarDetailsActivity"/>
        <activity
            android:name="com.zaaach.citypicker.CityPickerActivity"
            android:theme="@style/CityPicker.NoActionBar"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="stateHidden|adjustPan"/>
        <activity android:name=".activity.MsgActivity"/>
        <activity android:name=".activity.DetailDeliverCar" android:launchMode="singleTop"/>
        <activity android:name=".activity.DetailDeliverGoods" android:launchMode="singleTop"/>
        <activity android:name=".activity.DetailYunDan" android:launchMode="singleTop"/>
        <activity android:name=".activity.DetailYunJia" android:launchMode="singleTop"/>
        <activity android:name=".activity.ListDetailActivity" android:launchMode="singleTop"/>
    </application>

</manifest>